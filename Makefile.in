	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(baseboardSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(baseboarddir)/$$f'"; \
